local Tab = Window:CreateTab("Op things", 4483362458) -- Title, Image
local Section = Tab:CreateSection(".")
local Divider = Tab:CreateDivider()

local Players = game:GetService("Players")
local Ball = game.Workspace:FindFirstChild("Football")
local teleportCFrame = CFrame.new(319.823425, 10.8505239, -49.222084)
local myPlayer = Players.LocalPlayer
local teleportEnabled = false -- Toggle state

local Toggle = Tab:CreateToggle({
    Name = "insta goal-for away",
    CurrentValue = false,
    Flag = "FootballTeleportToggle",
    Callback = function(state)
        teleportEnabled = state
    end
})

local function onBallTouched(hit)
    if not teleportEnabled then return end

    local character = hit.Parent
    local player = Players:GetPlayerFromCharacter(character)

    if player and player.Team == myPlayer.Team then
        Ball.CFrame = teleportCFrame
        Ball.Velocity = Vector3.zero -- Stop movement
    end
end

if Ball then
    Ball.Touched:Connect(onBallTouched)
end

local Players = game:GetService("Players")
local Ball = game.Workspace:FindFirstChild("Football")
local teleportCFrame = CFrame.new(-242.376602, 10.8505239, -49.222084)
local myPlayer = Players.LocalPlayer
local teleportEnabled = false -- Toggle state

local Toggle = Tab:CreateToggle({
    Name = "Insta goal -for home",
    CurrentValue = false,
    Flag = "FootballTeleportToggle",
    Callback = function(state)
        teleportEnabled = state
    end
})

local function onBallTouched(hit)
    if not teleportEnabled then return end

    local character = hit.Parent
    local player = Players:GetPlayerFromCharacter(character)

    if player and player.Team == myPlayer.Team then
        Ball.CFrame = teleportCFrame
        Ball.Velocity = Vector3.zero -- Stop movement
    end
end

if Ball then
    Ball.Touched:Connect(onBallTouched)
end

local Button = Tab:CreateButton({
   Name = "Semi-prodigy",
   Callback = function()
   game:GetService("Players").LocalPlayer.PlayerStats.Flow.Value="Prodigy"
   end,
})

local Toggle = Tab:CreateToggle({
    Name = "auto lvl-away",
    CurrentValue = false,
    Flag = "TeleportToggle",
    Callback = function(Value)
        if Value then
            -- Define target object name and destination CFrame
            local targetObjectName = "Football"
            local destinationCFrame = CFrame.new(319.823425, 10.85055239, -49.222084)

            -- Get the player's character
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()

            -- Function to teleport the character to a given CFrame
            local function teleportToCFrame(cframe)
                if character and character:FindFirstChild("HumanoidRootPart") then
                    character.HumanoidRootPart.CFrame = cframe
                else
                    warn("HumanoidRootPart not found.")
                end
            end

            -- Find the target object in the workspace
            local targetObject = game.Workspace:FindFirstChild(targetObjectName)

            if targetObject then
                -- Teleport to the target object's position
                teleportToCFrame(targetObject.CFrame)
                wait(1) -- Wait 1 second before next teleport
                -- Teleport to the destination CFrame
                teleportToCFrame(destinationCFrame)
            else
                warn("Target object '" .. targetObjectName .. "' not found in Workspace.")
            end
        end
    end
})
